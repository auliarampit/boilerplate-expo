# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Google OAuth Configuration
# Get these from Google Cloud Console: https://console.cloud.google.com/
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your_google_web_client_id_here
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=your_google_ios_client_id_here

# Facebook OAuth Configuration
# Get this from Facebook Developers: https://developers.facebook.com/
EXPO_PUBLIC_FACEBOOK_CLIENT_ID=your_facebook_client_id_here
EXPO_PUBLIC_FACEBOOK_API_VERSION=v18.0
EXPO_PUBLIC_FACEBOOK_GRAPH_URL=https://graph.facebook.com
EXPO_PUBLIC_FACEBOOK_OAUTH_URL=https://www.facebook.com

# Sentry Configuration (optional)
# Get this from Sentry: https://sentry.io/
EXPO_PUBLIC_SENTRY_DSN=your_sentry_dsn_here

# API Configuration
EXPO_PUBLIC_API_BASE_URL_DEV=http://localhost:3000/api
EXPO_PUBLIC_API_BASE_URL_PROD=https://api.yourapp.com
EXPO_PUBLIC_API_TIMEOUT=10000

# Development Configuration
EXPO_PUBLIC_ENABLE_MOCK_API=true
EXPO_PUBLIC_ENABLE_FLIPPER=false
EXPO_PUBLIC_LOG_LEVEL=info

# Push Notifications Configuration
# Get this from your Expo project settings
EXPO_PUBLIC_PROJECT_ID=your_expo_project_id_here

# Additional API Keys (add as needed)
# EXPO_PUBLIC_ANALYTICS_KEY=your_analytics_key_here
# EXPO_PUBLIC_PUSH_NOTIFICATION_KEY=your_push_notification_key_here